package com.ict.edu2;

public class Test04 {
	public static void main(String[] args) {
		// 카페모카 6,500이다.
		// 친구와 둘이서 15,000원을 내고 주문했다
		// 잔돈은 얼마인가?(단, 부가세 10%포함)

		int su1 = 6500;
		int su2 = su1*2;
		int su3 = su2 + (int)(su2*0.1) ; //14300
		int su4 = 15000-su3 ;
		
		System.out.println(su3);
	}
}
